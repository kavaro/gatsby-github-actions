{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "firebase": "^7.3.0",
    "firebase-admin": "^8.7.0",
    "firebase-functions": "^3.3.0",
    "gatsby": "^2.17.6",
    "gatsby-image": "^2.2.30",
    "gatsby-plugin-manifest": "^2.2.25",
    "gatsby-plugin-offline": "^3.0.17",
    "gatsby-plugin-react-helmet": "^3.1.13",
    "gatsby-plugin-sharp": "^2.2.34",
    "gatsby-source-filesystem": "^2.1.35",
    "gatsby-transformer-sharp": "^2.3.1",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "firebase-tools": "^7.7.0",
    "gatsby-cli": "^2.8.8",
    "prettier": "^1.18.2"
  },
  "license": "MIT",
  "scripts": {
    "build": "node_modules/.bin/gatsby build",
    "develop": "node_modules/.bin/gatsby develop",
    "publish": "node_modules/.bin/firebase deploy",
    "test-publish": "curl -v POST -H \"Accept: application/vnd.github.everest-preview+json\" -H \"Content-Type: application/json\" --data '{\"event_type\": \"push\"}' \"https://api.github.com/repos/kavaro/gatsby-github-actions/dispatches\"",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "yarn develop",
    "serve": "node_modules/.bin/gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  }
}
